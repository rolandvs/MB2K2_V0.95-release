* MB2K2 hardware addresses* PIAKEYREG   equ   $FF00PIACA    equ   $FF01SYSREG   equ   $FF02PIACB    equ   $FF03** DUARTACIAD2   equ   $FF04ACIAC2   equ   $FF05ACIAD1   equ   $FF08ACIAC1   equ   $FF09BAUD1    equ   $FF0CBAUD2    equ   $FF0D** FDCCOMREG   equ   $FF10TRKREG   equ   $FF11SECREG   equ   $FF12DATREG   equ   $FF13** GDCGDCPRM   equ   $FF14GDCCOM   equ   $FF15** RTCRTCADD   equ   $FF18RTCDAT   equ   $FF19** RDC (ramdisk controller)RCOMREG   equ   $FF20RTRKREG   equ   $FF21RSECREG   equ   $FF22RDATREG   equ   $FF23RMODREG   equ   $FF24** PDC (promdisk controller)PCOMREG   equ   $FF30PTRKREG   equ   $FF31PSECREG   equ   $FF32PDATREG   equ   $FF33PMODREG   equ   $FF34** TMR (10Hz timer)TMRREG   equ   $FF40*